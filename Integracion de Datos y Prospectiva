        "df_referencia = df[df['Warehouse_block'].isin(['A','B','C'])]\n",
        "\n",
        "for column in df_referencia.columns:\n",
        "    print(f\"Información para la columna: {column}\")\n",
        "    if pd.api.types.is_numeric_dtype(df_referencia[column]):\n",
        "        print(f\"  La media es: {np.mean(df_referencia[column])}\")\n",
        "        print(f\"  La varianza es: {np.var(df_referencia[column])}\")\n",
        "        print(f\"  El coeficiente de asimetría es: {skew(df_referencia[column])}\")\n",
        "        print(f\"  El coeficiente de curtosis es: {kurtosis(df_referencia[column])}\")\n",
        "    else:\n",
        "        print(f\"  Value counts:\\n{df_referencia[column].value_counts()}\")\n",
        "    print(\"-\" * 30)"
